package examples;

class  Sum {
	 
	 static int x;
	 static int y;
	 

	 void Setsum (int value1, int value2)

	 {
		 x= value1;
		 y=value2;
//		 System.out.println(x);
	 }

static	int getSum()
	 {
		 return x+y;
	 }
 }

