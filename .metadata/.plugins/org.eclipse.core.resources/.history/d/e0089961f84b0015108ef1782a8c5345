package examples;

public class Inheritance_Example_01 {

	public static void main(String[] args) {
		
		Person person = new Person("Test Name");
		
	}

}


class Person
{
	String name;
	int age;
	
	public Person(String name)
	{
		this.name=name;
	}
	
	public void printName()
	{
		System.out.println(name);
	}
	
	
}
