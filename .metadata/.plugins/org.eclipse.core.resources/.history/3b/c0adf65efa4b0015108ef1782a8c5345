package example1;

public class InterfaceExample {

	public static void main(String[] args) {


	}

}


class Person
{
	String name;
	String address;
	
	public void printinfo(String name, String address)
	{
		this.name = name;
		this.address= address;
		
		System.out.println("Name "+name+'\n'+"Address "+address);
		
	}
	
}