package examples;

public class DynamicMethodDispatch {

	public static void main(String[] args) {
		
		Labrodor lab = new Labrodor();
		lab.breed="lab";
		lab.color="brown";
		lab.name="Nick";
		lab.weight=16;
	}

}

class Animal
{
	String color;
	int weight;
}

class Dog extends Animal
{
	String breed;
}

class Labrodor extends Dog
{
	String name;
	
	
}

