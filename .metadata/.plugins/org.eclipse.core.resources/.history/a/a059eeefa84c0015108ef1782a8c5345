package example1;

public class Inheritance04 {
	public static void main (String args[])
	{
		Cons3 c3 = new Cons3();
	
	}

}


class Cons1
{
	public Cons1()
	{
		System.out.println("From Con1");
	}
}

class Cons2 extends Cons1
{
	public Cons2()
	{
		System.out.println("From Con2");
	}
	
}

class Cons3 extends Cons2
{
	
	public Cons3()
	{
		System.out.println("From Con3");
	}
}