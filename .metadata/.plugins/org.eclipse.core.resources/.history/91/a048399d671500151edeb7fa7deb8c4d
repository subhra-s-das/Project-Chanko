	{
		int SingleArray [] = new int [3]; // can be used in a constructor

		SingleArray [0] = 1;
		
		
	}
	
	int SingleArray [] = new int [3]; // can be used in a constructor
	{
	SingleArray [0] = 1;
	}
	
