package example1;

public class Inheritance03 {

	public static void main(String[] args) {
		
		Car c1 = new Car();
		c1.typeinfo("Sedan");
		
		Car c2 = new Car();
		c2.typeinfo("Hatchback");
	}

}


class Car
{
	String type;
	String color;
	double weight;
	
	public void typeinfo(String type)
	{
		this.type=type;
		System.out.println(type);
	}
}